{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/certmanager/browser",
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/_proxy/api/$1" },
    { "src": "^/auth/(.*)$", "dest": "/api/_proxy/auth/$1" },
    { "handle": "filesystem" },
    { "src": "^(.*)$", "dest": "/index.html" }
  ]
}


